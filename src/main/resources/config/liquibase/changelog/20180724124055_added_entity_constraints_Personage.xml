<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Personage.
    -->
    <changeSet id="20180724124055-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="country_id"
                                 baseTableName="personage"
                                 constraintName="fk_personage_country_id"
                                 referencedColumnNames="id"
                                 referencedTableName="country"/>

        <addForeignKeyConstraint baseColumnNames="licensor_id"
                                 baseTableName="personage"
                                 constraintName="fk_personage_licensor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="licensor"/>

        <addForeignKeyConstraint baseColumnNames="history_id"
                                 baseTableName="personage"
                                 constraintName="fk_personage_history_id"
                                 referencedColumnNames="id"
                                 referencedTableName="history"/>

    </changeSet>
</databaseChangeLog>
